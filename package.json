{
  "name": "react-intro",
  "version": "0.0.0",
  "description": "Following Facebook React Tutorial",
  "main": "server.js",
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "body-parser": "^1.4.3",
    "express": "^4.4.5",
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  },
  "scripts": {
    "build": "npm run test && npm run build:verbose",
    "build:html": "node tools/build-html.js",
    "build:sass": "node-sass --quiet --output dist/styles public/styles/main.scss --source-map true --output-style compressed",
    "build:verbose": "node tools/build.js",
    "dev": "node tools/build-dev.js",
    "reset-dist": "npm run remove-dist && npm run create-dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "create-dist": "mkdir dist && cd dist && mkdir js && mkdir styles",
    "start": "parallelshell \"npm run test:watch\" \"gulp\"",
    "test": "NODE_ENV=test mocha --reporter progress --recursive \"./test/**/*-test.js\"",
    "test:watch": "npm run test --watch"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.0",
    "browser-sync": "^2.11.0",
    "chai": "^3.4.1",
    "cheerio": "^0.19.0",
    "colors": "^1.1.2",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.15.0",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.0",
    "gulp-nodemon": "^2.0.6",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "parallelshell": "^2.0.0",
    "react-redux": "^4.0.6",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "redux": "^3.0.5",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "watch": "^0.17.1",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "yargs": "^3.31.0"
  }
}
